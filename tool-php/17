function delay ($delay){
	for($tt = $delay ;$tt>= 1;$tt--){
        echo "\r\033[1;33m   LMK-TOOL \033[1;31m ~~>       \033[1;32m Äang         \033[1;31m | $tt | "; usleep(150000);
        echo "\r\033[1;31m   LMK-TOOL \033[0;33m   ~~>     \033[0;37m Äang L       \033[0;31m | $tt | "; usleep(150000);
        echo "\r\033[1;32m   LMK-TOOL \033[0;33m     ~~>   \033[0;37m Äang Láº¥      \033[0;31m | $tt | "; usleep(150000);
        echo "\r\033[1;34m   LMK-TOOL \033[0;33m       ~~> \033[0;37m Äang Láº¥y     \033[0;31m | $tt | "; usleep(150000);
        echo "\r\033[1;35m   LMK-TOOL \033[0;33m        ~~>\033[0;37m Äang Láº¥y J   \033[0;31m | $tt | "; usleep(150000);
        echo "\r\033[1;35m   LMK-TOOL \033[0;33m        ~~>\033[0;37m Äang Láº¥y Jo  \033[0;31m | $tt | "; usleep(150000);
        echo "\r\033[1;35m   LMK-TOOL \033[0;33m        ~~>\033[0;37m Äang Láº¥y Job.\033[0;31m | $tt | ";usleep(150000);} 
echo "\r\e[1;95m    ğŸ”¥LMK-TOOLğŸ”¥                       \r"; }
