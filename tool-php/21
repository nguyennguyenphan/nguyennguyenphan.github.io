function chay($so){
  for($v=0;$v<= $so;$v++){
    echo "\033[1;36m"."â–¬";usleep(20000);echo "\033[1;37m"."â–¬";usleep(20000);echo "\033[1;36m"."â–¬";usleep(20000);echo "\033[1;37m"."â–¬";usleep(20000);
