function delay($delay){
for($time=$delay;$time>-1;$time--){ 
	    echo "\r"; 
        echo "\r\033[1;36m   >\033[1;31m[\033[1;31m|\033[1;37m]\033[1;36m< \033[1;30m~\033[1;33m>      \033[1;33mL    \033[1;31m| \033[1;33m$time \033[1;31m|        ";
        usleep(150000);
        echo "\r"; 
        echo "\r\033[1;36m   >\033[1;37m[\033[1;31m/\033[1;37m]\033[1;36m<  \033[1;31m~\033[1;33m>     \033[1;35mLO   \033[1;31m| \033[1;33m$time \033[1;31m|        ";
        usleep(150000);
        echo "\r"; 
        echo "\r\033[1;36m   >\033[1;37m[\033[1;31m-\033[1;37m]\033[1;36m<   \033[1;32m~\033[1;33m>    \033[1;36mLOA  \033[1;31m| \033[1;33m$time \033[1;31m| \033[1;31mâ—      ";
        usleep(150000);
        echo "\r"; 
        echo "\r\033[1;36m   >\033[1;37m[\033[1;31m\]\033[1;36m<    ~\033[1;33m>   \033[1;37mLOAD \033[1;31m| \033[1;33m$time \033[1;31m| â—\033[1;33mâ—     ";
        usleep(150000);
        echo "\r"; 
        echo "\r\033[1;36m   >\033[1;37m[\033[1;31m|\033[1;37m]\033[1;36m<     \033[1;33m~\033[1;33m>  \033[1;32mLOAD \033[1;31m| \033[1;33m$time \033[1;31m| â—â—\033[1;35mâ—    ";
        usleep(150000);
