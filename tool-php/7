function echonhanxu($xu,$loai,$id,$soxu,$so){
  $time = date('H:i:s');
  $rd = rand(1, 7);
  if(strlen($id) >= 12){
    $id = substr($id, 0,15);
  }
if(strlen($loai) == 3){$kc = '   ';}else if(strlen($loai) == 4){$kc = '  ';}else if(strlen($loai) == 5){$kc = ' ';}else{$kc = '';}

$string = "\033[1;33m[\033[1;33m$so\033[1;33m] \033[1;31m√ó \033[1;36m$time \033[1;31m√ó \033[1;3".$rd."m$loai \033[1;31m√ó \033[1;37m$id \033[1;31m√ó \033[1;32m+$xu \033[1;31m√ó \033[1;33m$soxu\n";
for($i = 0;$i<= strlen($string);$i++){
  echo $string[$i];
  usleep(5000);
}
}
