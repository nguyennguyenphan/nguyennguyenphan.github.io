	$kl = "\e[1;32mâŒ \e[1;33mLMK-Tool\e[1;32mâŒ¡\e[1;35mâ¯\e[1;36mâ¯\e[1;31mâ¯\033[1;34m[\033[1;33m".$dem."\033[1;34m]\033[1;91m â— \033[1;36m".date("H:i:s")."\033[1;31m â— \033[1;".rand(31,37)."m".$loai."\033[1;31m â— \033[1;37m".$id."\033[1;31m â— \033[1;32m$xujob \033[1;31mâ—\033[1;32m $xu \n";
for($i = 0; $i < strlen($kl); $i++){echo $kl[$i];usleep(500);}
}
